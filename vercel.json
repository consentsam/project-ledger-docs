{
    "version": 2,
    "builds": [
        {
            "src": "static/**",
            "use": "@vercel/static"
        },
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/health",
            "dest": "/api/index.js",
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
                "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept, Authorization"
            }
        },
        {
            "src": "/api/register",
            "dest": "/api/register.js",
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
                "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept, Authorization"
            }
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/index.js",
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
                "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept, Authorization"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/static/$1",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        },
        {
            "src": "/",
            "dest": "/static/index.html",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ],
    "public": true
}