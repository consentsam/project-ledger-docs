{
    "version": 2,
    "builds": [
        {
            "src": "static/**",
            "use": "@vercel/static"
        },
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/",
            "dest": "/static/index.html"
        }
    ]
}